{
  "name": "nest-starter",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "packages/*",
    "apps/*",
    "modules/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --parallel run dev",
    "start": "pnpm -r --parallel run start",
    "lint": "eslint .",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.29.1",
    "supertest": "^6.3.4",
    "tsx": "^4.19.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.6"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.5",
    "@nestjs/core": "^10.4.5",
    "@nestjs/platform-express": "^10.4.5",
    "commander": "^12.1.0",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  }
}
